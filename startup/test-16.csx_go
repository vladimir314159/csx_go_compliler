package p16csx
func main() {
    var i int = 1;
    var j int = 1;
    var sum int = 0;
    print "Testing program p16csx\n";
    for i <= 100 {
        j = 1;
        for j <= 100 {
            j = j + 1;
            sum = sum + 1;
        } // for
        i = i + 1;
    } // for
    print "The value of sum is ", sum, "\n";
} // func main
// package p16csx
