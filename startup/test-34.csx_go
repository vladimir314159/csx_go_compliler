  Checks for proper handling of cast operations.

##

package p34csx
func main() {
    var d char;
    print "Testing Program p34csx\n";
    d = char(98 + 256);
    print "character should be b: ", d, '\n';
    print "should be 1: ", int(1), '\n';
    print "should be false: ", bool(0), '\n';
    print "should be true: ", bool(1), '\n';
    print "should be 0: ", int(1 == 4), '\n';
    print "should be 98: ", int('b'), '\n';
    print "Test completed\n";
} // func main
// package p34csx
