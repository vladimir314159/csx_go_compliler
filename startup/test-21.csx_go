## Test 21
 Created: Dec 2002; Modified Jun 2018

 Check control flow in a deeply nested loop

##

package p21csx
func main() {
    var loopcount int;
    var flag bool;
    print "Testing Program p21csx\n";
    flag = true;
    loopcount = 0;
    for flag == true {
        loopcount = loopcount + 1;
        for flag == true {
            loopcount = loopcount + 1;
            for flag == true {
                loopcount = loopcount + 1;
                for flag == true {
                    loopcount = loopcount + 1;
                    for flag == true {
                        loopcount = loopcount + 1;
                        for flag == true {
                            loopcount = loopcount + 1;
                            for flag == true {
                                loopcount = loopcount + 1;
                                for flag == true {
                                    loopcount = loopcount + 1;
                                    for flag == true {
                                        loopcount = loopcount + 1;
                                        for flag == true {
                                            loopcount = loopcount + 1;
                                            flag = false;
                                        } // for
                                    } // for
                                } // for
                            } // for
                        } // for
                    } // for
                } // for
            } // for
        } // for
    } // for
    if loopcount != 10 {
        print "\nERROR: Not all loops executed properly: ", loopcount, "\n";
    }
    print "Test completed\n";
} // func main
// package p21csx
